/*Shows maximum pressure in January 2013*/
SELECT pressure_millibar, daymonth, year FROM PakistanClimate
WHERE year = '2013' and daymonth LIKE '%Jan%' ORDER BY pressure_millibar;

/*Shows maximum pressure in Febuary 2013*/
SELECT pressure_millibar, daymonth, year FROM PakistanClimate
WHERE year = '2013' and daymonth LIKE '%Feb%' ORDER BY pressure_millibar;

