/*Shows maximum pressure in January 2009*/
SELECT pressure_millibar, daymonth, year FROM PakistanClimate
WHERE year = '2009' and daymonth LIKE '%Jan%' ORDER BY pressure_millibar;

/*Shows maximum pressure in Febuary 2009*/
SELECT pressure_millibar, daymonth, year FROM PakistanClimate
WHERE year = '2009' and daymonth LIKE '%Feb%' ORDER BY pressure_millibar;

