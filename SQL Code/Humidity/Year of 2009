/*Shows  maximum humidity in January 2009*/
SELECT humidity_percentage, daymonth, year FROM PakistanClimate
WHERE year = '2009' and daymonth LIKE '%Jan%' ORDER BY humidity_percentage;

/*Shows  maximum humidity in Feburary 2009*/
SELECT humidity_percentage, daymonth, year FROM PakistanClimate
WHERE year = '2009' and daymonth LIKE '%Feb%' ORDER BY humidity_percentage;

