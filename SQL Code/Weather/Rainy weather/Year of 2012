/*Shows rainy weather in days of January*/
SELECT weather, daymonth, year FROM PakistanClimate
WHERE weather LIKE '%rain%' AND daymonth LIKE '%Jan%' AND year = '2012';

/*Shows rainy weather in days of Febuary*/
SELECT weather, daymonth, year FROM PakistanClimate
WHERE weather LIKE '%rain%' AND daymonth LIKE '%Feb%' AND year = '2012';

/*Shows rainy weather in days of March*/
SELECT weather, daymonth, year FROM PakistanClimate
WHERE weather LIKE '%rain%' AND daymonth LIKE '%Mar%' AND year = '2012';

