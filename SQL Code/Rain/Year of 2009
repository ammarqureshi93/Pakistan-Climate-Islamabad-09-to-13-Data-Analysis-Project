/*Shows  highest rain values in January 2009*/
SELECT rain_mm, daymonth, year FROM PakistanClimate
WHERE year = '2009' and daymonth LIKE '%Jan%' ORDER BY rain_mm;

/*Shows  highest rain values in Feburary 2009*/
SELECT rain_mm, daymonth, year FROM PakistanClimate
WHERE year = '2009' and daymonth LIKE '%Feb%' ORDER BY rain_mm;

