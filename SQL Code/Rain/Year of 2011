/*Shows  highest rain values in January 2011*/
SELECT rain_mm, daymonth, year FROM PakistanClimate
WHERE year = '2011' and daymonth LIKE '%Jan%' ORDER BY rain_mm;

/*Shows  highest rain values in Feburary 2011*/
SELECT rain_mm, daymonth, year FROM PakistanClimate
WHERE year = '2011' and daymonth LIKE '%Feb%' ORDER BY rain_mm;

