/*Shows  highest rain values in January 2012*/
SELECT rain_mm, daymonth, year FROM PakistanClimate
WHERE year = '2012' and daymonth LIKE '%Jan%' ORDER BY rain_mm;

/*Shows  highest rain values in Feburary 2012*/
SELECT rain_mm, daymonth, year FROM PakistanClimate
WHERE year = '2012' and daymonth LIKE '%Feb%' ORDER BY rain_mm;

