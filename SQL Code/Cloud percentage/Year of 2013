/*Shows maximum cloud in January 2013*/
SELECT cloud_percentage, daymonth, year FROM PakistanClimate
WHERE year = '2013' and daymonth LIKE '%Jan%' ORDER BY cloud_percentage;

/*Shows maximum cloud in Febuary 2013*/
SELECT cloud_percentage, daymonth, year FROM PakistanClimate
WHERE year = '2013' and daymonth LIKE '%Feb%' ORDER BY cloud_percentage;

