/*Shows  highest wind values in January 2010*/
SELECT wind_kmh, daymonth, year FROM PakistanClimate
WHERE year = '2010' and daymonth LIKE '%Jan%' ORDER BY wind_kmh;

/*Shows  highest wind values in Feburary 2010*/
SELECT wind_kmh, daymonth, year FROM PakistanClimate
WHERE year = '2010' and daymonth LIKE '%Feb%' ORDER BY wind_kmh;

