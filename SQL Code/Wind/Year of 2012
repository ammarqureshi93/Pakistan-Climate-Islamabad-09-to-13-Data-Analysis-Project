/*Shows  highest wind values in January 2012*/
SELECT wind_kmh, daymonth, year FROM PakistanClimate
WHERE year = '2012' and daymonth LIKE '%Jan%' ORDER BY wind_kmh;

/*Shows  highest wind values in Feburary 2012*/
SELECT wind_kmh, daymonth, year FROM PakistanClimate
WHERE year = '2012' and daymonth LIKE '%Feb%' ORDER BY wind_kmh;

