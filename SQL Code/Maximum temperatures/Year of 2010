/*Shows maximum temperature in January 2010*/
SELECT max_temp_Celsius, daymonth, year FROM PakistanClimate
WHERE year = '2010' and daymonth LIKE '%Jan%' ORDER BY max_temp_Celsius;

/*Shows maximum temperature in Febuary 2010*/
SELECT max_temp_Celsius, daymonth, year FROM PakistanClimate
WHERE year = '2010' and daymonth LIKE '%Feb%' ORDER BY max_temp_Celsius;

